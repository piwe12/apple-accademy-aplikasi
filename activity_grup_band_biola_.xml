        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        app:srcCompat="@mipmap/banner_chainsmokers_foreground" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="670dp"
        android:layout_height="769dp"
        android:layout_marginBottom="200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/banner_chainsmokers_foreground" />

    <ImageButton
        android:id="@+id/banner_cold"
        android:layout_width="382dp"
        android:layout_height="137dp"
        android:layout_marginTop="450dp"
        android:layout_marginEnd="550dp"
        android:layout_marginRight="550dp"
        android:background="#000000"
        app:layout_constraintBottom_toTopOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/banner_chain"
        app:srcCompat="@mipmap/banner_coldplay_foreground" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="670dp"
        android:layout_height="769dp"
        android:layout_marginTop="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/banner_coldplay_foreground" />

    <ImageButton
        android:id="@+id/banner_maron"
        android:layout_width="389dp"
        android:layout_height="139dp"
        android:layout_marginTop="800dp"
        android:layout_marginEnd="550dp"
        android:layout_marginRight="550dp"
        android:layout_marginBottom="27dp"
        android:background="#000000"
        app:layout_constraintBottom_toTopOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/banner_cold"
        app:srcCompat="@mipmap/banner_marron_5_foreground" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="670dp"
        android:layout_height="769dp"
        android:layout_marginTop="380dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/banner_marron_5_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>
