<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#050505"
    tools:context=".grup_band_piano">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="586dp"
        android:layout_height="529dp"
        android:layout_marginBottom="430dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/background2_foreground" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="84dp"
        android:layout_height="84dp"
        app:srcCompat="@mipmap/ikonnisulo_foreground" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="112dp"
        android:layout_height="59dp"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="20dp"
        android:text="Nisulo"
        android:textColor="#FFFDFD"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="350dp"
        android:layout_marginLeft="350dp"
        android:layout_marginTop="550dp"
        android:text="Pilih Group Band Mu"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/banner_chain"
        android:layout_width="378dp"
        android:layout_height="137dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="500dp"
        android:layout_marginRight="500dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@mipmap/banner_chainsmokers_foreground" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="670dp"
        android:layout_height="769dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/banner_chainsmokers_foreground" />

    <ImageButton
        android:id="@+id/banner_cold"
        android:layout_width="378dp"
        android:layout_height="144dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="550dp"
        android:layout_marginRight="550dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/banner_chain"
        app:srcCompat="@mipmap/banner_coldplay_foreground" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="670dp"
        android:layout_height="769dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/banner_coldplay_foreground" />

    <ImageButton
        android:id="@+id/banner_maroon"
        android:layout_width="378dp"
        android:layout_height="132dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="390dp"
        android:layout_marginRight="390dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/banner_cold"
        app:layout_constraintTop_toBottomOf="@+id/banner_cold"
        app:srcCompat="@mipmap/banner_marron_5_foreground" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="670dp"
        android:layout_height="769dp"
        android:layout_marginTop="150dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/banner_marron_5_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>
