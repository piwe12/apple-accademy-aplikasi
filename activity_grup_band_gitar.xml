<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#050505"
    tools:context=".grup_band_gitar">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="586dp"
        android:layout_height="529dp"
        android:layout_marginBottom="430dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/background2_foreground" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="84dp"
        android:layout_height="84dp"
        app:srcCompat="@mipmap/ikonnisulo_foreground" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="112dp"
        android:layout_height="59dp"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="20dp"
        android:text="Nisulo"
        android:textColor="#FFFDFD"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="250dp"
        android:text="Pilih Group Band Mu"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/banner_chain"
        android:layout_width="374dp"
        android:layout_height="127dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="540dp"
        android:layout_marginRight="540dp"
        android:background="#000000"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@mipmap/banner_chainsmokers_foreground" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="669dp"
        android:layout_height="769dp"
        android:layout_marginBottom="200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.521"
        app:srcCompat="@mipmap/banner_chainsmokers_foreground" />

    <ImageButton
        android:id="@+id/banner_cold"
        android:layout_width="374dp"
        android:layout_height="130dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="540dp"
        android:layout_marginRight="540dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/banner_chain"
        app:srcCompat="@mipmap/banner_coldplay_foreground" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="670dp"
        android:layout_height="899dp"
        android:layout_marginTop="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/banner_coldplay_foreground" />

    <ImageButton
        android:id="@+id/banner_maroon"
        android:layout_width="378dp"
        android:layout_height="141dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="550dp"
        android:layout_marginRight="550dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/banner_cold"
        app:srcCompat="@mipmap/banner_marron_5_foreground" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="670dp"
        android:layout_height="958dp"
        android:layout_marginTop="320dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:srcCompat="@mipmap/banner_marron_5_foreground" />


</androidx.constraintlayout.widget.ConstraintLayout>
